{
    "version": 2,
    "builds": [
      {
        "src": "src/server-local.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "src/**/*.ts",
            "src/**/*.json",
            ".env"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "src/server-local.ts"
      }
    ]
  }
  